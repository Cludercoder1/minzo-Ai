{
  "name": "minzofoundation-ai",
  "version": "1.0.0",
  "description": "MinzoFoundation AI Platform - Advanced Generative AI Solutions",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run client\"",
